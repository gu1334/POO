import java.io.IOException;

public class Cachorro
{
    String raca;
    int tamanho;
    String cor;
    boolean temRabo;
    
    
    
    void late(String raca, int tamanho, String cor, String rabo){
        
        System.out.println("Minha raça é " + raca + " tenho " + tamanho + " metros " + " sou " + cor +" e " + mensagem);
        
    } 
    public static String mensagem(boolean temRabo){
        
        if(temRabo == true){
            String mensagem =  " tenho rabo";
            return  mensagem;
        }
        else{
        
             String mensagem =  " Não tenho rabo";
            return  mensagem;
        }
    }
    
	public static void main(String[] args) {

    Cachorro dog1 = new Cachorro();
    dog1.raca = "Golden";
    dog1.tamanho = 3;
    dog1.cor = "Amarelo";
    dog1.temRabo = false;
    
    dog1.late(dog1.raca, dog1.tamanho, dog1.cor, dog1.mensagem);

  Cachorro dog2 = new Cachorro();
    dog2.raca = "Golden";
    dog2.tamanho = 3;
    dog2.cor = "Amarelo";
    dog2.temRabo = false;
    
    dog2.late(dog2.raca, dog2.tamanho, dog2.cor, dog2.mensagem);


	}
}
